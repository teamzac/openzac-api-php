{
    "name": "teamzac/openzac-api-php",
    "description": "Official PHP API client for openzac.com",
    "version": "2.0",
    "keywords": ["open data"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com"
        }
    ],
    "require": {
        "php": ">=5.7",
        "nesbot/carbon": "~1.20",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "5.4.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "TeamZac\\OpenZac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamZac\\OpenZac\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors"
    }
}